<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
		package="com.android.keystore"
		android:versionCode="2"
		android:versionName="1.1">
	<application android:label="@string/app_name">
		<activity android:name="MyKeyStoreActivity"
				android:theme="@style/Theme.LargeFont"
				android:screenOrientation="sensor"
				android:icon="@drawable/mot_passe_icone_7117_32"
				android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
			<intent-filter>
				<action android:name="com.android.keystore.MyKeyStoreActivity.SIGN" />
				<category android:name="android.intent.category.DEFAULT"/>
			</intent-filter>
			<intent-filter>
				<action android:name="com.android.keystore.MyKeyStoreActivity.VERIFY" />
				<category android:name="android.intent.category.DEFAULT"/>
			</intent-filter>
			<intent-filter>
				<action android:name="com.android.keystore.MyKeyStoreActivity.ENCRYPT_WITH_PUBKEY" />
				<category android:name="android.intent.category.DEFAULT"/>
			</intent-filter>
			<intent-filter>
				<action android:name="com.android.keystore.MyKeyStoreActivity.ENCRYPT_WITH_PRIVKEY" />
				<category android:name="android.intent.category.DEFAULT"/>
			</intent-filter>
			<intent-filter>
				<action android:name="com.android.keystore.MyKeyStoreActivity.DECRYPT_WITH_PUBKEY" />
				<category android:name="android.intent.category.DEFAULT"/>
			</intent-filter>
			<intent-filter>
				<action android:name="com.android.keystore.MyKeyStoreActivity.DECRYPT_WITH_PRIVKEY" />
				<category android:name="android.intent.category.DEFAULT"/>
			</intent-filter>
		</activity>
		<activity android:name=".GenerateKeyActivity"
				android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		<activity android:name=".WebViewActivity"
				android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		<activity android:name=".TextViewActivity"
				android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		<activity android:name="PreferencesActivity"
				android:label="@string/preferences_titre">
		</activity>
	</application>
</manifest> 
